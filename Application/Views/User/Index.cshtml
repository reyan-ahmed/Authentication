@model List<Identity.Entities.ApplicationUser>;



<h2>List of User</h2>
<div class="table-responsive">
    <table class="table table-striped table-sm">
        <thead>
            <tr>
                <th scope="col">Id</th>
                <th scope="col">UserName</th>
                <th scope="col">Email</th>
                <th scope="col">IsEnabled</th>
                <th scope="col">FullName</th>
                <th scope="col"></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.UserName</td>
                    <td>@item.Email</td>
                    <td>@item.IsEnabled</td>
                    <td>@item.FullName</td>
                    <td>
                        @Html.ActionLink("Delete", "Delete", "User", new { id = item.Id }) |
                        @Html.ActionLink("Update", "Update", "User", new { userId = item.Id })
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>


